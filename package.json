{
  "name": "playwrigth-e2e-tests",
  "version": "1.0.0",
  "description": "practices for playwright using typescript",
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "clean:report": "del /s /q  allure-results\\*.*",
    "api-e2e-testing":"playwright test  --config=config/test.playwright.config.ts tests/api/user.api.test.ts",
    "e2e:make-appoint": "playwright test --config=config/test.playwright.config.ts tests/e2e/make.appointment.e2e.test.ts --project=chromium --headed",
    "e2e-NopComm-HomePage":"playwright test  --config=config/test.playwright.config.ts tests/e2e/nopcommerce.e2e.test.ts --project=chromium --headed",
    "Dev:make-appoint": "playwright test  --config=config/dev.playwright.config.ts tests/functional/Make.app.test.ts --project=chromium --headed",
    "test:make-appoint": "playwright test  --config=config/test.playwright.config.ts tests/functional/Make.app.test.ts --project=chromium --headed",
    "demo": "playwright test tests/demo/mytest.test.ts --headed",
    "demo-paralles": "playwright test --grep=@demo",
    "demo-ConstantData": "playwright test --grep=@constantData",
    "demo-paramTestData": "playwright test tests/demo/paramaterData.test.ts --headed",
    "csv-As-param-TestData": "playwright test --config=config/test.playwright.config.ts tests/functional/csv-make-apt.test.ts --headed",
    "demo-CookiesSetUp-GlobalData": "playwright test tests/demo/CookiesSetup.test.ts --headed",
    "screen-shot": "npm run clean:report && playwright test tests/functional/screenshort-Video.test.ts.test.ts --project=chromium --headed",
    "make-appoint": "playwright test tests/functional/Make.app.test.ts --project=chromium --headed",
    "make-APT-ON-Emulator": "playwright test tests/functional/EmulatorDevice.test.ts --headed",
    "Debuge-appoint": "playwright test tests/demo/Debug.Demo.test.ts --project=chromium --headed",
    "Debuge-appoint:ui": "playwright test tests/demo/Debug.Demo.test.ts --project=chromium --ui --headed",
    "Debuge-appoint:trace": "playwright test tests/demo/Debug.Demo.test.ts --project=chromium --trace on",
    "Debuge-appoint:cli": "PWDEBUG=1 npx playwright test tests/demo/Debug.Demo.test.ts --project=chromium",
    "ele-Handle": "playwright test tests/demo/element.Handling.Make.app.test.ts --project=chromium --headed",
    "HTMLreportrun": "playwright show-report",
    "inventory-Demo": "npm run clean:report && playwright test tests/functional/inventory.test.ts --project=chromium --headed",
    "AnnotationWithSkip": "npm run clean:report && playwright test tests/functional/annotationDemoAndSkip.test.ts  --headed",
    "Demo-Smoke-Tags": "npm run clean:report && playwright test --grep=@smoke  --headed",
    "Demo-Regression-Tags": "set DEBUG=pw:api && npm run clean:report && playwright test --grep=@Regression  --headed",
    "pw-add-cap": "set DEBUG=pw:api && npm run clean:report && playwright test tests/demopw.Add.Capp.test.ts --project=chromium --headed",
    "browser-Maximize": "set DEBUG=pw:api && npm run clean:report && playwright test tests/demo/Browser-Setup.test.ts --project=chromium --headed",
    "Demo-Hooks": "set DEBUG=pw:api && npm run clean:report && playwright test tests/demo/Demo-Hooks.test.ts --project=chromium --headed",
    "Demo-global_Remove_report_file": "playwright test tests/demo/Make.app.test.ts --project=chromium"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.1",
    "@types/node": "^25.2.0",
    "allure-playwright": "^3.4.5",
    "chalk": "^5.6.2",
    "csv-parse": "^6.1.0",
    "dotenv": "^17.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "csv": "^6.4.1",
    "D": "^1.0.0",
    "parse": "^8.1.0",
    "rimraf": "^6.1.2"
  }
}
